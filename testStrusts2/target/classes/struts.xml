<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.5//EN"
	"http://struts.apache.org/dtds/struts-2.5.dtd">
<struts>

	<constant name="struts.enable.DynamicMethodInvocation"
		value="false" />
	<constant name="struts.devMode" value="true" />

	<package name="Authentif" extends="struts-default" strict-method-invocation="true" namespace="/">
		<action name="register" class="com.aplose.actions.Register">
			<result name="success">/WEB-INF/thankyou.jsp</result>
			<result name="error">/WEB-INF/error.jsp</result>
		</action>
		<action name="registration">
			<result name="success">/WEB-INF/register.jsp</result>
		</action>

	</package>

</struts>
